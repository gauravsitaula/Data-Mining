64889     +%macro row_pivot_normalize(transds=,outtransds=,row=,col=,entry=,
64890     +                           col_sumds=, pivot=.5, tmt_config= , tmt_train=1, prefix=);
64892     +   /* Calculate sum of the squared entries for each row */
64893     +proc summary nway data=&transds;
64894     +   class &row;
64895     +   var &entry;
64896     +   output out=_sqrowvals uss=;
64897     +   run;
64899     +   /* Put into &meandiv what the average euclidean length is across rows */
64902     +%if &tmt_train = 1  %then %do;
64903     +   proc sql noprint;
64904     +      select mean(sqrt(&entry)) into :meaneuclen
64905     +      from _sqrowvals;
64906     +   quit;
64907     +   %if &tmt_config ne %then %do;
64908     +      *populate the config file with the mean value;
64909     +      data &tmt_config;
64910     +         set &tmt_config;
64911     +         &prefix._meaneuclen= symget('meaneuclen');
64912     +      run;
64913     +   %end;
64914     +    data _sqrowvals;
64915     +      set _sqrowvals;
64916     +      meaneuclen=symget('meaneuclen');
64917     +      divisor = meaneuclen + (sqrt(&entry) - meaneuclen)*&pivot;
64918     +      drop meaneuclen;
64919     +   run;
64922     +%end;
64923     +%else %do;
64924     +      * grab the mean value from the config file  and put into meaneuclien;
64925     +   data _null_;
64926     +      set &tmt_config;
64927     +      call symput('meaneuclen',&prefix._meaneuclen);
64928     +   run;
64929     +    data _sqrowvals;
64930     +      set _sqrowvals;
64931     +      meaneuclen=symget('meaneuclen');
64932     +      divisor = meaneuclen + (sqrt(&entry) - meaneuclen)*&pivot;
64933     +   run;
64935     +%end;
