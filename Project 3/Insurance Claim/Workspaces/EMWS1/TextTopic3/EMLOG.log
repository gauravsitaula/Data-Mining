66361     +%macro apply_labels(inds,labelds,label_col=_name,col_id=_topicid,prefix=COL,outds=);
66362     +%if &outds= %then %let outds=&inds;
66363     +proc sql noprint;
66364     +    select max(&col_id), min(&col_id) into :_maxvar, :_minvar from &labelds;
66365     +       quit;
66366     +%if &_minvar eq . %then %let _minvar=1;
66367     +%let _minvar=%left(&_minvar);
66368     +%let _maxvar=%left(&_maxvar);
