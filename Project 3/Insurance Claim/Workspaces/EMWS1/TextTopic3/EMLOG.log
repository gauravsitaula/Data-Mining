66371     +%if &_maxvar >0 %then %do;
66372     +
66373     +%let _minlab=%ktrim(_tmlab)&_minvar;
66374     +%let _maxlab=%ktrim(_tmlab)&_maxvar;
66375     +proc sql noprint;
66376     +    select &label_col into :&_minlab - :&_maxlab from &labelds;
66377     +       quit;
66378     +data &outds;
66379     +   set &inds;
66380     +   array vars{&_minvar:&_maxvar} &prefix.&_minvar-&prefix.&_maxvar;
