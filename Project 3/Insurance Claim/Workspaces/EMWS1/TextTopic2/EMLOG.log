54150     +%macro row_pivot_normalize(transds=,outtransds=,row=,col=,entry=,
54151     +                           col_sumds=, pivot=.5, tmt_config= , tmt_train=1, prefix=);
54153     +   /* Calculate sum of the squared entries for each row */
54154     +proc summary nway data=&transds;
54155     +   class &row;
54156     +   var &entry;
54157     +   output out=_sqrowvals uss=;
54158     +   run;
54160     +   /* Put into &meandiv what the average euclidean length is across rows */
54163     +%if &tmt_train = 1  %then %do;
54164     +   proc sql noprint;
54165     +      select mean(sqrt(&entry)) into :meaneuclen
54166     +      from _sqrowvals;
54167     +   quit;
54168     +   %if &tmt_config ne %then %do;
54169     +      *populate the config file with the mean value;
54170     +      data &tmt_config;
54171     +         set &tmt_config;
54172     +         &prefix._meaneuclen= symget('meaneuclen');
54173     +      run;
54174     +   %end;
54175     +    data _sqrowvals;
54176     +      set _sqrowvals;
54177     +      meaneuclen=symget('meaneuclen');
54178     +      divisor = meaneuclen + (sqrt(&entry) - meaneuclen)*&pivot;
54179     +      drop meaneuclen;
54180     +   run;
54183     +%end;
54184     +%else %do;
54185     +      * grab the mean value from the config file  and put into meaneuclien;
54186     +   data _null_;
54187     +      set &tmt_config;
54188     +      call symput('meaneuclen',&prefix._meaneuclen);
54189     +   run;
54190     +    data _sqrowvals;
54191     +      set _sqrowvals;
54192     +      meaneuclen=symget('meaneuclen');
54193     +      divisor = meaneuclen + (sqrt(&entry) - meaneuclen)*&pivot;
54194     +   run;
54196     +%end;
54201     +proc sql noprint;
54202     +   create table &outtransds as
54203     +      select a.&row,a.&col,a.&entry / divisor as &entry
54204     +      from &transds as a,_sqrowvals as b
54205     +      where a.&row=b.&row;
54206     +   drop table _sqrowvals;
54207     +         quit;
54208     +%if &col_sumds ne %then %do;
54209     +   proc summary nway data=&outtransds;
54210     +   class &col;
54211     +   var &entry;
54212     +   output out=&col_sumds mean=;
54213     +   run;
54214     +%end;
