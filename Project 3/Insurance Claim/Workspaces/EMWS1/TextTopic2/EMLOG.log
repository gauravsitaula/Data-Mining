57350     +
57351     +%let _minlab=%ktrim(_tmlab)&_minvar;
57352     +%let _maxlab=%ktrim(_tmlab)&_maxvar;
57353     +proc sql noprint;
57354     +    select &label_col into :&_minlab - :&_maxlab from &labelds;
57355     +       quit;
57356     +data &outds;
57357     +   set &inds;
