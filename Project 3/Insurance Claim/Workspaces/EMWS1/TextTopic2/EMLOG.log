57339     +%macro apply_labels(inds,labelds,label_col=_name,col_id=_topicid,prefix=COL,outds=);
57340     +%if &outds= %then %let outds=&inds;
57341     +proc sql noprint;
57342     +    select max(&col_id), min(&col_id) into :_maxvar, :_minvar from &labelds;
57343     +       quit;
57344     +%if &_minvar eq . %then %let _minvar=1;
