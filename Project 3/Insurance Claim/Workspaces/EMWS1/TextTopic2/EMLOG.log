55867     +%macro row_pivot_normalize(transds=,outtransds=,row=,col=,entry=,
55868     +                           col_sumds=, pivot=.5, tmt_config= , tmt_train=1, prefix=);
55870     +   /* Calculate sum of the squared entries for each row */
55871     +proc summary nway data=&transds;
55872     +   class &row;
55873     +   var &entry;
55874     +   output out=_sqrowvals uss=;
55875     +   run;
55877     +   /* Put into &meandiv what the average euclidean length is across rows */
55880     +%if &tmt_train = 1  %then %do;
55881     +   proc sql noprint;
55882     +      select mean(sqrt(&entry)) into :meaneuclen
55883     +      from _sqrowvals;
55884     +   quit;
55885     +   %if &tmt_config ne %then %do;
55886     +      *populate the config file with the mean value;
55887     +      data &tmt_config;
55888     +         set &tmt_config;
55889     +         &prefix._meaneuclen= symget('meaneuclen');
55890     +      run;
55891     +   %end;
55892     +    data _sqrowvals;
55893     +      set _sqrowvals;
55894     +      meaneuclen=symget('meaneuclen');
55895     +      divisor = meaneuclen + (sqrt(&entry) - meaneuclen)*&pivot;
55896     +      drop meaneuclen;
55897     +   run;
55900     +%end;
55901     +%else %do;
55902     +      * grab the mean value from the config file  and put into meaneuclien;
55903     +   data _null_;
55904     +      set &tmt_config;
55905     +      call symput('meaneuclen',&prefix._meaneuclen);
55906     +   run;
55907     +    data _sqrowvals;
55908     +      set _sqrowvals;
55909     +      meaneuclen=symget('meaneuclen');
55910     +      divisor = meaneuclen + (sqrt(&entry) - meaneuclen)*&pivot;
55911     +   run;
55913     +%end;
55918     +proc sql noprint;
55919     +   create table &outtransds as
55920     +      select a.&row,a.&col,a.&entry / divisor as &entry
55921     +      from &transds as a,_sqrowvals as b
