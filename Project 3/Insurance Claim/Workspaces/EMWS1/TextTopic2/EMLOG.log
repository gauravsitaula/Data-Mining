54318     +   proc sql noprint;
54319     +      create table _tmptop as
54320     +         select a.*, b._doccutoff, b._termcutoff
54321     +         from _tmptop as a left join &topic_cutoff_ds as b
54322     +         on upcase(a._topic_)=upcase(b._name);
54323     +            quit;
54324     +
54325     +   proc sql noprint;
54326     +      create table _termtop1  as
54327     +         select a._topic_,
54328     +            case
54329     +              when b.parent=. then b.key else b.parent end
54330     +              as _termid, a._weight_ as _weight, a._doccutoff, a._termcutoff
54331     +         from &termds as b,_tmpTop as a
54332     +         where (b.key ne b.parent) and (a._term_= ' ' and a._role_=b.role);
54333     +            quit;
54334     +   proc sql noprint;
54335     +      create table _termtop2  as
54336     +         select a._topic_,
54337     +            case
54338     +              when b.parent=. then b.key else b.parent end
54339     +              as _termid, a._weight_ as _weight, a._doccutoff, a._termcutoff
54340     +         from &termds as b,_tmpTop as a
54341     +         where (b.key ne b.parent) and
54342     +         (a._term_ ne ' ' and a._role_ = ' ' and a._term_=b.term);
54343     +            quit;
54344     +   proc sql noprint;
54345     +      create table _termtop3  as
