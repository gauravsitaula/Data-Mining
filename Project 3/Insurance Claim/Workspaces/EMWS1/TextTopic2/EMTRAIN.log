54360     +   proc sort data=&termtopicds; by _topic_;
54361     +
54362     +   /* Now create the topic data set, which has one row per topic, and
54363     +    the convert the termtopic data set to have one row per actual term
54364     +    per topic */
54365     +   data &topicds (keep=_topicid _name _displayCat _cat _docCutoff _termCutoff
