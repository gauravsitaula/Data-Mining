23417     +%macro row_pivot_normalize(transds=,outtransds=,row=,col=,entry=,
23418     +                           col_sumds=, pivot=.5, tmt_config= , tmt_train=1, prefix=);
23420     +   /* Calculate sum of the squared entries for each row */
23421     +proc summary nway data=&transds;
23422     +   class &row;
23423     +   var &entry;
23424     +   output out=_sqrowvals uss=;
23425     +   run;
23427     +   /* Put into &meandiv what the average euclidean length is across rows */
23430     +%if &tmt_train = 1  %then %do;
23431     +   proc sql noprint;
23432     +      select mean(sqrt(&entry)) into :meaneuclen
23433     +      from _sqrowvals;
23434     +   quit;
23435     +   %if &tmt_config ne %then %do;
23436     +      *populate the config file with the mean value;
23437     +      data &tmt_config;
23438     +         set &tmt_config;
23439     +         &prefix._meaneuclen= symget('meaneuclen');
23440     +      run;
23441     +   %end;
23442     +    data _sqrowvals;
23443     +      set _sqrowvals;
23444     +      meaneuclen=symget('meaneuclen');
23445     +      divisor = meaneuclen + (sqrt(&entry) - meaneuclen)*&pivot;
23446     +      drop meaneuclen;
23447     +   run;
23450     +%end;
23451     +%else %do;
23452     +      * grab the mean value from the config file  and put into meaneuclien;
23453     +   data _null_;
23454     +      set &tmt_config;
23455     +      call symput('meaneuclen',&prefix._meaneuclen);
23456     +   run;
23457     +    data _sqrowvals;
23458     +      set _sqrowvals;
23459     +      meaneuclen=symget('meaneuclen');
23460     +      divisor = meaneuclen + (sqrt(&entry) - meaneuclen)*&pivot;
23461     +   run;
23463     +%end;
23468     +proc sql noprint;
23469     +   create table &outtransds as
23470     +      select a.&row,a.&col,a.&entry / divisor as &entry
23471     +      from &transds as a,_sqrowvals as b
23472     +      where a.&row=b.&row;
23473     +   drop table _sqrowvals;
23474     +         quit;
23475     +%if &col_sumds ne %then %do;
23476     +   proc summary nway data=&outtransds;
23477     +   class &col;
23478     +   var &entry;
23479     +   output out=&col_sumds mean=;
23480     +   run;
23481     +%end;
